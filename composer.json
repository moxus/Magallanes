{
  "name": "andres-montanez/magallanes",
  "description": "The Deployment Tool for PHP Applications",
  "homepage": "https://magephp.com",
  "license": "MIT",
  "type": "library",
  "keywords": ["deployment"],
  "authors": [
    {
      "name": "Andrés Montañez",
      "email": "andresmontanez@gmail.com"
    },
    {
      "name": "Fritz Michael Gschwantner",
      "email": "fmg@inspiredminds.at"
    },
    {
      "name": "Christian Sagmüller",
      "email": "christian.sagmueller@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "monolog/monolog": "~1.11",
    "symfony/console": "^5.0",
    "symfony/filesystem": "^5.0",
    "symfony/event-dispatcher": "^5.0",
    "symfony/finder": "^5.0",
    "symfony/yaml": "^5.0",
    "symfony/process": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Mage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mage\\Tests\\": "tests/"
    }
  },
  "bin": ["bin/mage"],
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  }
}
